{
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "colorizedBracketPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    {
      "open": "\"",
      "close": "\"",
      "notIn": [
        "string"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "wordPattern": "([^\\s\\[\\]\\(\\)\\{\\}\"'`,;]+)",
  "indentationRules": {
    "increaseIndentPattern": "^.*(\\(|\\[|\\{)[^\\]\\)\\}]*$",
    "decreaseIndentPattern": "^\\s*[\\]\\)\\}]"
  },
  "folding": {
    "offSide": true
  }
}
